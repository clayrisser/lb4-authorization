{
  "name": "lb4-keycloak",
  "version": "0.0.1",
  "description": "keycloak adapter for loopback 4",
  "keywords": [
    "lb4-keycloak",
    "loopback",
    "lb4",
    "4",
    "keycloak",
    "oauth",
    "saml",
    "token",
    "introspection",
    "oauth",
    "auth",
    "openid",
    "server"
  ],
  "homepage": "https://github.com/codejamninja/lb4-keycloak",
  "bugs": {
    "url": "https://github.com/codejamninja/lb4-keycloak/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/lb4-keycloak"
  },
  "scripts": {
    "build": "npm run test && babel src -d lib --extensions \".ts,.tsx\" --source-maps inline && tsc -d --emitDeclarationOnly",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "clean:purge": "git clean -fXd",
    "format": "prettier --write ./**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx}",
    "lint": "npm run format && tsc --allowJs --noEmit && eslint --ext .ts,.tsx ./",
    "lint:fix": "npm run format && tsc --noEmit && eslint --ext .ts,.tsx --fix ./",
    "prepublish": "npm run build",
    "start": "babel-node src --extensions \".ts,.tsx\"",
    "test": "npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-preset-jam": "^0.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-config-jam": "^0.2.9",
    "tsc": "^1.20150623.0",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ]
}
